<h1>Song List</h1>

<%= link_to "Create Song", new_song_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Artist</th>
    <th>Slug</th>
    <th>Actions</th>
  </tr>
  <% @songs.each do |song| %>
    <tr>
      <td><%= link_to song.title, song_path(song.slug) %></td>
      <td><%= song.artist %></td>
      <td><%= song.slug %></td>
      <td>
        <%= link_to "Edit", edit_song_path(song.slug) %> | 
        <%= link_to "Destroy", song_path(song.slug), method: :delete %>
      </td>
    </tr>
  <% end %>
</table>
